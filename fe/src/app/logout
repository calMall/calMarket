    nickname
  }

const data = await fetch(`${url}/api/logout`, {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    credentials: "include",
    body : JSON.stringify(logoutInfo)
  });

  console.log(logoutInfo)
  console.log(data)
  if (!data.ok) throw new Error(data.statusText);
