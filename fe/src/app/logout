
const url = process.env.NEXT_PUBLIC_BASE_URL;

export const logout = async (logout: string): Promise<CheckLogoutRes> => {
  const data = await fetch(`${/api/logout}/api/users/check-logout?logout=${logout}`, {
    method: "POST",
    credentials: "include",
  });
  if (!data.ok) throw new Error(data.statusText);
  return data.json();
};

